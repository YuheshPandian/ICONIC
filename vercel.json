{
    "version": 2,
    "builds": [
        {
            "src": "api/vercel_python_handler.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/icons/(.*)",
            "dest": "/icons/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/vercel_python_handler.py"
        }
    ]
}
